	urldate = {2012-06-13},
	date = {2006-02-20},
	note = {00000},
	keywords = {Artificial neural networks, Black box and gray-box models, Conceptual models, Hydrologic modelling, Rainfall runoff modelling, Self-organizing networks},
	file = {ScienceDirect Full Text PDF:/Users/smwesten/Library/Application Support/Firefox/Profiles/bjp41trg.default/zotero/storage/NZJZNP67/Jain and Srinivasulu - 2006 - Integrated approach to model decomposed flow hydro.pdf:application/pdf;ScienceDirect Snapshot:/Users/smwesten/Library/Application Support/Firefox/Profiles/bjp41trg.default/zotero/storage/8HEW794H/S0022169405002854.html:text/html}
