@article{sophocleous_combining_1991,
	title = {Combining the soilwater balance and water-level fluctuation methods to estimate natural groundwater recharge. Practical aspects.},
	volume = {124},
	abstract = {A relatively simple and practical approach for calculating groundwater recharge in semiarid plain environments with a relatively shallow water table, such as the Kansas Prairies, has been developed. Major uncertainties in the Darcian, water balance, and groundwater fluctuation analysis approaches were examined, and a combination methodology for reducing some of the uncertainties is proposed. By combining a storm-based soil water balance (lasting several days) with the resulting water table rise, effective storativity values of the region near the water table are obtained. This combination method is termed the 'hybrid water-fluctuation method.' Using a simple average of several such estimates results in a site-calibrated effective storativity value that can be used to translate each major water table rise tied to a specific storm period into a corresponding amount of groundwater recharge. Estimation errors in the hybrid water-fluctuation method are reduced by running a 'storm period '-based soil water balance throughout the year in combination with the associated water level rise, and by employing the {CRAE} methodology for areal evapotranspiration estimates, thus avoiding the soil-plant system complexities. Examples of soil water balance and water level fluctuation analyses based on field-measured data from Kansas show that the proposed methodology gives better and more reliable results than either of the two well-established approaches used singly. (Author's abstract) 35 056308000},
	pages = {229--241},
	number = {3},
	journaltitle = {Journal of Hydrology(Amsterdam)},
	author = {Sophocleous, M. A},
	date = {1991},
	file = {[PDF] from ku.edu:/Users/smwesten/Library/Application Support/Firefox/Profiles/bjp41trg.default/zotero/storage/5RJXAGNI/Sophocleous - 1991 - Combining the soilwater balance and water-level fl.pdf:application/pdf;Snapshot:/Users/smwesten/Library/Application Support/Firefox/Profiles/bjp41trg.default/zotero/storage/7IIK36AZ/002216949190016B.html:text/html}
