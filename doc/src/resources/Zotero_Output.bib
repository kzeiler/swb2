	file = {Full Text PDF:/Users/smwesten/Library/Application Support/Firefox/Profiles/bjp41trg.default/zotero/storage/KQZFQHXS/Tedela et al. - 2012 - Runoff Curve Numbers for 10 Small Forested Watersh.pdf:application/pdf;Snapshot:/Users/smwesten/Library/Application Support/Firefox/Profiles/bjp41trg.default/zotero/storage/QHVNKNAD/(ASCE)HE.1943-5584.html:text/html}
