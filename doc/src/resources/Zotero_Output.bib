@article{yearsley_semi-lagrangian_2009,
	title = {A semi-Lagrangian water temperature model for advection-dominated river systems},
	volume = {45},
	rights = {© 2008 American Geophysical Union},
	issn = {0043-1397},
	url = {http://www.agu.org.ezproxy.library.wisc.edu/pubs/crossref/2009/2008WR007629.shtml},
	doi = {10.1029/2008WR007629},
	abstract = {This paper describes a one-dimensional stream temperature model that is computationally efficient and highly scalable in both time and space. The model is developed within the framework of state space structure. The time-dependent equations for the conservation of thermal energy in a flowing stream or river are solved using a mixed Eulerian-Lagrangian, or semi-Lagrangian, numerical scheme. Solutions are obtained by tracking individual water parcels along their flow characteristics and storing the simulated results at discrete points on a fixed grid. Computational efficiency and accuracy of the numerical scheme are demonstrated by comparison of model estimates with observations of stream temperatures from rivers in the Pacific Northwest as well as with results from a closed-form solution of the energy equation. A preliminary analysis of the impact of climate changes on stream temperature in the Columbia River system illustrates the strengths of the semi-Lagrangian method for addressing water quality issues of regional, national, and, ultimately, global scale. Further development of the semi-Lagrangian method has the potential to improve the ability of water quality planners to perform uncertainty analysis, risk analysis, and forecasting for large, complex river systems.},
	pages = {W12405},
	number = {12},
	journaltitle = {Water Resources Research},
	shortjournal = {Water Resour. Res.},
	author = {Yearsley, John R.},
	urldate = {2012-12-07},
	date = {2009-12-08},
	keywords = {computational efficiency, energy budget, mathematical model, semi-Lagrangian, state space},
	file = {Full Text PDF:/Users/smwesten/Library/Application Support/Firefox/Profiles/bjp41trg.default/zotero/storage/G7KRN5K6/Yearsley - 2009 - A semi-Lagrangian water temperature model for adve.pdf:application/pdf;Full Text PDF:/Users/smwesten/Library/Application Support/Firefox/Profiles/bjp41trg.default/zotero/storage/MGBANH22/Yearsley - 2009 - A semi-Lagrangian water temperature model for adve.pdf:application/pdf;Snapshot:/Users/smwesten/Library/Application Support/Firefox/Profiles/bjp41trg.default/zotero/storage/9RZEHK5J/2008WR007629.html:text/html;Snapshot:/Users/smwesten/Library/Application Support/Firefox/Profiles/bjp41trg.default/zotero/storage/P3ZTV8CH/abstract.html:text/html}
