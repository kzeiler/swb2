	note = {00008},
	file = {Full Text PDF:/Users/smwesten/Library/Application Support/Firefox/Profiles/bjp41trg.default/zotero/storage/HNTMDR88/Stewart et al. - 2012 - Curve Number Determination Methods and Uncertainty.pdf:application/pdf;Snapshot:/Users/smwesten/Library/Application Support/Firefox/Profiles/bjp41trg.default/zotero/storage/JXKHWUW4/(ASCE)HE.1943-5584.html:text/html}
}

@article{jena_rs_2012,
	title = {{RS} and Geographical Information System–Based Evaluation of Distributed and Composite Curve Number Techniques},
