@article{zhang_explicitly_2011,
	title = {Explicitly integrating parameter, input, and structure uncertainties into Bayesian Neural Networks for probabilistic hydrologic forecasting},
	volume = {409},
	issn = {0022-1694},
	url = {http://www.sciencedirect.com/science/article/pii/S0022169411006238},
	doi = {10.1016/j.jhydrol.2011.09.002},
	abstract = {Summary
Estimating uncertainty of hydrologic forecasting is valuable to water resources and other relevant decision making processes. Recently, Bayesian Neural Networks ({BNNs}) have been proved powerful tools for quantifying uncertainty of streamflow forecasting. In this study, we propose a Markov Chain Monte Carlo ({MCMC}) framework ({BNN}-{PIS}) to incorporate the uncertainties associated with parameters, inputs, and structures into {BNNs}. This framework allows the structure of the neural networks to change by removing or adding connections between neurons and enables scaling of input data by using rainfall multipliers. The results show that the new {BNNs} outperform {BNNs} that only consider uncertainties associated with parameters and model structures. Critical evaluation of posterior distribution of neural network weights, number of effective connections, rainfall multipliers, and hyper-parameters shows that the assumptions held in our {BNNs} are not well supported. Further understanding of characteristics of and interactions among different uncertainty sources is expected to enhance the application of neural networks for uncertainty analysis of hydrologic forecasting.},
	pages = {696--709},
	number = {3},
	journaltitle = {Journal of Hydrology},
	shortjournal = {Journal of Hydrology},
	author = {Zhang, Xuesong and Liang, Faming and Yu, Beibei and Zong, Ziliang},
	urldate = {2012-10-25},
	date = {2011-11-09},
	keywords = {Bayesian Neural Networks, Evolutionary Monte Carlo, Hydrologic forecasting, Streamflow, Uncertainty},
	file = {ScienceDirect Full Text PDF:/Users/smwesten/Library/Application Support/Firefox/Profiles/bjp41trg.default/zotero/storage/U9ASFMPQ/Zhang et al. - 2011 - Explicitly integrating parameter, input, and struc.pdf:application/pdf;ScienceDirect Snapshot:/Users/smwesten/Library/Application Support/Firefox/Profiles/bjp41trg.default/zotero/storage/5RUI2VDS/S0022169411006238.html:text/html}
}

