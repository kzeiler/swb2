	file = {Full Text PDF:/Users/smwesten/Library/Application Support/Firefox/Profiles/bjp41trg.default/zotero/storage/SE6FHVFS/Francis and Vavrus - 2012 - Evidence linking Arctic amplification to extreme w.pdf:application/pdf;Snapshot:/Users/smwesten/Library/Application Support/Firefox/Profiles/bjp41trg.default/zotero/storage/74RTGQ7W/full.html:text/html}
