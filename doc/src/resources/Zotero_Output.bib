@article{barry_greenampt_2005,
	title = {Green–Ampt approximations},
	volume = {28},
	issn = {0309-1708},
	url = {http://www.sciencedirect.com/science/article/pii/S0309170805000898},
	doi = {10.1016/j.advwatres.2005.03.010},
	abstract = {The solution to the Green and Ampt infiltration equation is expressible in terms of the Lambert W−1 function. Approximations for Green and Ampt infiltration are thus derivable from approximations for the W−1 function and vice versa. An infinite family of asymptotic expansions to W−1 is presented. Although these expansions do not converge near the branch point of the W function (corresponds to Green–Ampt infiltration with immediate ponding), a method is presented for approximating W−1 that is exact at the branch point and asymptotically, with interpolation between these limits. Some existing and several new simple and compact yet robust approximations applicable to Green–Ampt infiltration and flux are presented, the most accurate of which has a maximum relative error of 5\&\#xa0;×\&\#xa0;10−5\%. This error is orders of magnitude lower than any existing analytical approximations.},
	pages = {1003--1009},
	number = {10},
	journaltitle = {Advances in Water Resources},
	author = {Barry, D.A. and Parlange, J.-Y. and Li, L. and Jeng, D.-S. and Crapper, M.},
	urldate = {2012-07-17},
	date = {2005-10},
	keywords = {Asymptotic expansions, Branch point, Iteration scheme, Lambert W−1 function approximations},
	file = {ScienceDirect Full Text PDF:/Users/smwesten/Library/Application Support/Firefox/Profiles/bjp41trg.default/zotero/storage/JRSIHD9P/Barry et al. - 2005 - Green–Ampt approximations.pdf:application/pdf;ScienceDirect Snapshot:/Users/smwesten/Library/Application Support/Firefox/Profiles/bjp41trg.default/zotero/storage/ACKGS5HS/S0309170805000898.html:text/html}
}

