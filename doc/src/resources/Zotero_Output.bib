@article{chapman_comparison_2002,
	title = {Comparison of models for estimation of groundwater recharge, using data from a deep weighing lysimeter},
	volume = {59},
	issn = {0378-4754},
	url = {http://www.sciencedirect.com/science/article/pii/S0378475401003895},
	doi = {10.1016/S0378-4754(01)00389-5},
	abstract = {Most conceptual models of catchment hydrology include algorithms for estimation of recharge to groundwater, which is then routed to stream base flow and/or deep regional groundwater. The majority of models attempt to simulate the process of percolation to groundwater from an overlying soil water store, which may be conceived as a single store or a series combination of stores. However, these models have seldom been tested against field observations, and therefore, the purpose of this study is to compare the performance of 13 single store and two-store models, selected from the literature, using an 11 years daily record from a weighing lysimeter 2.4\&\#xa0;m deep at Coshocton, {OH}, supporting a pasture grass. The percolation data are measured directly, while the volume of water in the soil column is measured by weighing the monolith every 5\&\#xa0;min. Use of data from three adjacent non-weighing lysimeters enabled an assessment of the spatial variability of groundwater recharge. The results of the study show that five models perform well in simulating recharge to a level consistent with spatial variability over a scale of a few metres, but did not perform as well in simulating variations of total soil water in the column, particularly during periods of high evapotranspiration. All the models, however, tend to underestimate the sharp peaks in recharge which may occur in extreme events.},
	pages = {3--17},
	number = {1},
	journaltitle = {Mathematics and Computers in Simulation},
	author = {Chapman, T.G. and Malone, R.W.},
	urldate = {2012-04-04},
	date = {2002-05-10},
	keywords = {Infiltration, lysimeter, Percolation, Recharge models, Soil water},
	file = {ScienceDirect Full Text PDF:/Users/smwesten/Library/Application Support/Firefox/Profiles/bjp41trg.default/zotero/storage/FX946TQT/Chapman and Malone - 2002 - Comparison of models for estimation of groundwater.pdf:application/pdf;ScienceDirect Snapshot:/Users/smwesten/Library/Application Support/Firefox/Profiles/bjp41trg.default/zotero/storage/G3TJN2Q5/Chapman and Malone - 2002 - Comparison of models for estimation of groundwater.html:text/html}
